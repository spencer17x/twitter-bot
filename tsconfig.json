{
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist",
    "target": "es2016",
    "module": "commonjs",
    "allowImportingTsExtensions": false,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "declaration": true
  }
}
